# Copy this file to .env and fill your values
GOOGLE_CLOUD_PROJECT=your-project-id
GCP_REGION=us-central1
GCS_BUCKET=your-bucket-name
DATASET_PATH=data/sample_reviews.csv            # local path OR gs://bucket/path.csv
