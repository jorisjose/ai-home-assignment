{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run: main (local CSV)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "args": ["--limit", "10"]
    },
    {
      "name": "Run: agent question",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "args": ["--agent", "What are customers most upset about?"]
    }
  ]
}
